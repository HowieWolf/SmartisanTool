<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="Widget.SeekBar.Normal" parent="@android:style/Widget.SeekBar">
        <item name="android:progressDrawable">@drawable/progress_bar</item>
        <item name="android:thumb">@drawable/scrubber_control_normal_smartisanos</item>
        <item name="android:thumbOffset">10.0dip</item>
    </style>

</resources>
